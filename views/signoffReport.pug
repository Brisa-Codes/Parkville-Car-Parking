doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Signedoff Cars || Parking
  script(src='https://kit.fontawesome.com/ec067d744e.js' crossorigin='anonymous')
  link(rel='stylesheet' href='/CSS/styles.css')
.parking-nav
  .nav-wrapper
    a.nav-link-logo(href='/')
      i.fas.fa-car
      span Parkville
  .nav-wrapper-mid
    a.nav-link(href='/dashboard')
      span Dashboard
  .nav-wrapper-log
    a(href='/')
      i.fas.fa-power-off
      span LogOut
.parking-report
  .parking-report-details 
    h3.parking-title Car Parking Entries
    h3
      button.btn.btn-primary
        a.nav-link(href='/signoffUser') New Entry
  table.parking-table(style='width:100%')
    tr
      th Name
      th Receipt No
      th Phone Number
      th Time
      th Gender 
      th UserNIN
    
    //- if signoffUsers.length 
    //- each signoffUser in signoffUsers
     tr
      td #{signoffUser.name}
      td #{signoffUser.receiptno}
      td #{signoffUser.phonenumber}
      td #{signoffUser.time}
      td #{signoffUser.gender}
      td #{signoffUser.nin}
     tr